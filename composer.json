{
    "name": "laminas-api-tools/api-tools-api-problem",
    "description": "Laminas Module providing API-Problem assets and rendering",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "rest",
        "api-problem"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-api-problem/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-api-problem",
        "rss": "https://github.com/laminas-api-tools/api-tools-api-problem/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "laminas/laminas-eventmanager": "~2.3",
        "laminas/laminas-http": "~2.3",
        "laminas/laminas-json": "~2.3",
        "laminas/laminas-mvc": "~2.3",
        "laminas/laminas-view": "~2.3",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-console": "~2.3",
        "laminas/laminas-loader": "~2.3",
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\ApiProblem\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ApiTools\\ApiProblem\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-api-problem": "self.version"
    }
}
